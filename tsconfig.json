{
    "compileOnSave": true,
    "compilerOptions": {
        "target": "es5",
        "forceConsistentCasingInFileNames": false,
        "noFallthroughCasesInSwitch": true,
        "module": "commonjs",
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "strictNullChecks": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "experimentalDecorators": true,
        "jsx": "react"
    },
    "exclude": [
        "node_modules",
        "typings",
        "wwwroot",
        "example-tag.html.tsx",
        "old"
    ]
}